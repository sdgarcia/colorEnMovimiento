<body style="margin-top:120px">
<h3> <%= @city.name %> </h3>
<div style="float:right" class="btn btn-link"><%= link_to 'Volver', admin_path %></div> 
</br>	
<div class="table table-striped">
  <table>
    <thead>
    	<tr>
    		<th>Email</th>
    		<th>Otra<th>
        <th>&nbsp;</th>
    	</tr>
    </thead>
    <tbody>
    	<% @votes.each do |vote| %>
        <tr>
        	<td><%= vote.email %></td>
        	<td><%= vote.other %></td>
          <td>
            <button class="btn btn-danger btn-small" onclick="window.location='?delete_vote=<%= vote.id %>' ;">Eliminar</button>
          </td>
        </tr>
  	  <% end %>
    </tbody>
  </table>
</div>
</body>