<body style="margin-top:120px">

<h3>Listado de Votos</h3></br>
<div style="float:right" class="btn btn-link"><%= link_to 'Volver', admin_path %></div> 
<div class="table table-striped">
<table >
  <tr>
    <th>Email</th>
    <th>City</th>
    <th>Other</th>
  </tr>

<% @votes.each do |vote| %>
  <tr>
    <td><%= vote.email %></td>
    <td><%= vote.city.name %></td>
    <td><%= vote.other %></td>
  </tr>
<% end %>
</table>

</body>
