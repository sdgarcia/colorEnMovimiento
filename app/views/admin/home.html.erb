<body style="margin-top:120px">

<h3>Dashboard</h3></br>
<div style="float:right" class="btn btn-link"><%= link_to 'Ciudades', cities_path %></div> 
<div class="table table-striped">
<table>
  <tr>
    <th>Ciudad</th>
    <th>Votos<th>
  </tr>
  <% @votes.each do |vote| %>
  <tr>
    <td><%= City.find(vote[0]).name %></td>
    <td style="text-align:right"><% if (vote[0] == 6) %> 
      <%= @votes_other %>
      <% else %>
      <%= vote[1] %>
      <% end %></td>
    <td><%= link_to "Detalle", :controller => "votes_admin", :action => "detail", :id => vote[0] %></td>
  </tr>
   <% end %>
   <tr>
    <td>Total</td>
    <td style="text-align:right"><%= @total_votes %></td>
    <td><%= link_to "Detalle", :controller => "votes_admin", :action => "index" %></td>
   </tr>
 </table>
<div>

</body>